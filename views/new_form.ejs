<body>
  <%- include('header', { user: user ? { is_admin: user.is_admin } : null }) %>


  <h1>New Form</h1>

  <form action="/add" method="post" id="new-product-form" class="new-product-form">
    <div class="new-product-form__field-wrapper">
      <label for="new-product-form__title" class="form-label">Title</label>
      <input type="text" name="title" placeholder="Title" id="new-product-form__title" class="form-control" style='background-color:rgb(239, 236, 236)'>
    </div>

    <div class="new-product-form__field-wrapper">
      <label for="new-product-form__description" class="form-label">Description</label>
      <textarea placeholder="Description" name="description" id="new-product-form__description" cols="30" rows="" class="form-control" style='background-color:rgb(239, 236, 236)'></textarea>
    </div>

    <div class="new-product-form__field-wrapper">
      <label for="new-product-form__price" class="form-label">Price</label>
      <input placeholder="Price" type="number" name="price" id="new-product-form__price" class="form-control" style='background-color:rgb(239, 236, 236)'>
    </div>

    <div class="new-product-form__field-wrapper">
      <label for="new-product-form__photo_url_1" class="form-label">Image</label>
      <input placeholder="Furniture Image" type="text" name="photo_url_1" id="new-product-form__photo_url_1" class="form-control" style='background-color:rgb(239, 236, 236)'>
    </div>

    <div class="new-product-form__field-wrapper">
      <label for="new-product-form__photo_url_2" class="form-label">Image</label>
      <input placeholder="Furniture Image" type="text" name="photo_url_2" id="new-product-form__photo_url_2" class="form-control" style='background-color:rgb(239, 236, 236)'>
    </div>

    <div class="new-product-form__field-wrapper">
      <label for="new-product-form__photo_url_3" class="form-label">Image</label>
      <input placeholder="Furniture Image" type="text" name="photo_url_3" id="new-product-form__photo_url_3" class="form-control" style='background-color:rgb(239, 236, 236)'>
    </div>

    <div class="new-product-form__field-wrapper">
      <label for="new-product-form__stock_quantity" class="form-label">Quantity</label>
      <input placeholder="Quantity" type="number" name="stock_quantity" id="new-product-form__stock_quantity" class="form-control" style='background-color:rgb(239, 236, 236)'/>
    </div>

    <div class="new-product-form__field-wrapper">
      <button type="submit" class="add-button btn btn-dark mb-3">Add</button>

    </div>

  </form>

  <script src="scripts/new_product_form.js"></script>
